# Пример файла .env для проекта
# 
# Для использования: скопируйте этот файл в .env и заполните своими значениями
# 
# НИКОГДА не коммитьте .env файл в репозиторий!
# Файл .env уже добавлен в .gitignore

# Конфигурация Google OAuth
# Получить можно в Google Cloud Console в разделе "Учетные данные"
# Используется для валидации Google ID токенов из Flutter приложения
GOOGLE_MOBILE_CLIENT_ID=ваш_google_mobile_client_id
GOOGLE_OAUTH2_CLIENT_ID=ваш_google_oauth2_client_id

# Ключ безопасности Django (для продакшена)
# Генерируется случайным образом, например через:
# python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'
SECRET_KEY=ваш_секретный_ключ

# Настройки отладки
# Установите False для продакшена
DEBUG=True

# Настройки базы данных (по умолчанию используется SQLite)
# Раскомментируйте и заполните, если используете другую БД
# DB_ENGINE=django.db.backends.postgresql
# DB_NAME=имя_вашей_базы
# DB_USER=пользователь_базы
# DB_PASSWORD=пароль_базы
# DB_HOST=localhost
# DB_PORT=5432